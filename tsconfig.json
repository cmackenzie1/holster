{
	"compilerOptions": {
		"skipLibCheck": true,
		"module": "es2022",
		"target": "es2022",
		"lib": ["es2021"],
		"sourceMap": true,
		"moduleResolution": "node",
		"esModuleInterop": true,
		"types": [
			"@cloudflare/workers-types",
